{
  "name": "cf-workflow-rollback",
  "version": "1.0.0",
  "description": "Saga-pattern rollback utility for Cloudflare Workflows",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "workflow",
    "saga",
    "rollback",
    "compensation",
    "durable"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@types/bun": "latest",
    "tsdown": "^0.4.0",
    "typescript": "^5.0.0"
  }
}
